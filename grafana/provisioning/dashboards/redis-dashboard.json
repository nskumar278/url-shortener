{
  "id": null,
  "title": "Redis Metrics",
  "tags": ["redis", "cache"],
  "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Connected Clients",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Max Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
        "yAxes": [
          {"label": "bytes", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "yAxes": [
          {"label": "percent", "min": 0, "max": 100},
          {"show": false}
        ]
      },
      {
        "id": 4,
        "title": "Commands per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Key Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(redis_keyspace_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Total Keys",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_db_keys",
            "legendFormat": "Total Keys"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Expired Keys",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_expired_keys_total[5m])",
            "legendFormat": "Expired Keys/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 18, "x": 6, "y": 16}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s"
}
