{
    "id": null,
    "title": "URL Shortener Performance Dashboard",
    "tags": [
        "url-shortener",
        "performance"
    ],
    "timezone": "browser",
    "panels": [
        {
            "id": 1,
            "title": "Request Rate (req/s)",
            "type": "stat",
            "targets": [
                {
                    "expr": "sum(rate(url_shortener_http_requests_total[1m]))",
                    "legendFormat": "Requests/sec",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "red",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 800
                            },
                            {
                                "color": "green",
                                "value": 1000
                            }
                        ]
                    },
                    "unit": "reqps"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 0,
                "y": 0
            }
        },
        {
            "id": 2,
            "title": "Response Time (95th percentile)",
            "type": "stat",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "95th percentile",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.03
                            },
                            {
                                "color": "red",
                                "value": 0.05
                            }
                        ]
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 6,
                "y": 0
            }
        },
        {
            "id": 3,
            "title": "Cache Hit Ratio",
            "type": "stat",
            "targets": [
                {
                    "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m]))",
                    "legendFormat": "Hit Ratio",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "red",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.8
                            },
                            {
                                "color": "green",
                                "value": 0.9
                            }
                        ]
                    },
                    "unit": "percentunit"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 12,
                "y": 0
            }
        },
        {
            "id": 4,
            "title": "Error Rate",
            "type": "stat",
            "targets": [
                {
                    "expr": "sum(rate(url_shortener_http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(url_shortener_http_requests_total[5m]))",
                    "legendFormat": "Error Rate",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.01
                            },
                            {
                                "color": "red",
                                "value": 0.05
                            }
                        ]
                    },
                    "unit": "percentunit"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 18,
                "y": 0
            }
        },
        {
            "id": 5,
            "title": "Response Time Distribution",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "50th percentile",
                    "datasource": "Prometheus"
                },
                {
                    "expr": "histogram_quantile(0.95, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "95th percentile",
                    "datasource": "Prometheus"
                },
                {
                    "expr": "histogram_quantile(0.99, sum(rate(url_shortener_http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "99th percentile",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "pointSize": 3,
                        "showPoints": "never"
                    }
                }
            },
            "options": {
                "tooltip": {
                    "mode": "multi",
                    "sort": "desc"
                },
                "legend": {
                    "displayMode": "table",
                    "placement": "bottom",
                    "calcs": ["lastNotNull", "max", "mean"]
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            }
        },
        {
            "id": 6,
            "title": "CPU Usage by App",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum by (name) (rate(container_cpu_usage_seconds_total{name=~\"url-shortener-dev-app[1-3]\"}[5m])) * 100",
                    "legendFormat": "{{ name }}",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100,
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "pointSize": 3,
                        "showPoints": "never"
                    }
                }
            },
            "options": {
                "tooltip": {
                    "mode": "multi",
                    "sort": "desc"
                },
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom",
                    "calcs": ["lastNotNull"]
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            }
        },
        {
            "id": 7,
            "title": "Memory Usage by App",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum by (name) (container_memory_usage_bytes{name=~\"url-shortener-dev-app[1-3]\"})",
                    "legendFormat": "{{ name }}",
                    "datasource": "Prometheus"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "bytes",
                    "min": 0,
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "smooth",
                        "pointSize": 3,
                        "showPoints": "never"
                    }
                }
            },
            "options": {
                "tooltip": {
                    "mode": "multi",
                    "sort": "desc"
                },
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom",
                    "calcs": ["lastNotNull"]
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            }
        },
        {
            "id": 8,
            "title": "Request Status Codes",
            "type": "piechart",
            "targets": [
                {
                    "expr": "sum by (status_code) (rate(url_shortener_http_requests_total[5m]))",
                    "legendFormat": "{{ status_code }}",
                    "datasource": "Prometheus"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            }
        }
    ],
    "time": {
        "from": "now-15m",
        "to": "now"
    },
    "refresh": "30s",
    "timepicker": {
        "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
        "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
}
